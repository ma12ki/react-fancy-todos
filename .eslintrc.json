{
  "plugins": [
    "react",
    "destructuring"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:destructuring/recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "jest": true
  }
}
